@model User
<br />
<div class="container row p-0 m-0">
    <div class="col-6">
        <h2 class="text-info">User List</h2>
        <div id="xxx">
        </div>

    </div>
    <div class="col-3 offset-3">
        <a asp-action="Upsert" asp-controller="Users" class="btn btn-info form-control text-white">
            Add New User
        </a>
    </div>

    <div class="col-12 border p-3">
        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Username</th>
                    <th>Password</th>
                    <th>RememberMe</th>
                    <th>ReturnUrl</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{
    <script src="~/js/userList.js"></script>
}